<!doctype html>

<html>

  <head>
    
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=1065,user-scalable=yes">

<script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js" type="text/javascript"></script>
<script src="/scripts/jquery.nivo.slider.pack.js" type="text/javascript"></script>

<script type="text/javascript">
    $(function() {
		
		/* active sidebar links */
		$("#sidebar h1 a, #sidebar .summary-title a").each(function(){ 
   			var currentNav = $(this);
   			var linkTxt = $(this).text();
   			$("#content h1:contains('" +linkTxt+ "')").each(function(){
      			$(currentNav).addClass("activeNav");
   			});
		});
                
    });
    </script>




    <!--INCLUDE SQS SCRIPTS, META TAGS AND USER CONTENT FROM THE CODE INJECTION TAB-->

    {squarespace-headers}
    <squarespace:script src="site.js" combo="true" />
  
  </head>

  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">
    
    <div id="canvasWrapper">
    <div id="canvas">
    
      <div id="flags">
      <a href=""><img src="/assets/Flag_Japan.jpg"></a>
      <a href="/"><img src="/assets/Flag_US.jpg"></a>
      </div>
    
    <!--HEADER-->
    <header id="header">

	<div id="headerLeft">
      <!--LOGO-->
      {.section website}
		<div id="logo">
        <h1 data-content-field="site-title">
          <a href="/">
            {.section logoImageUrl}
             <img src="{logoImageUrl}?format=original" alt="{siteTitle}" />
            {.or}
              {siteTitle}
            {.end}
          </a>
        </h1>
        </div>
    </div>
        
      
	<div id="headerRight">
      <!--MAIN NAVIGATION-->
      <squarespace:navigation navigationId="mainNav" template="navigation" />
      <!--SITE TAGLINE-->
      {.section siteTagLine}<div id="tagline">{@}</div>{.end}
      {.end}
    </div>


    </header>
    
    